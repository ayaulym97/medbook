                        _reactNative.Alert.alert("Пожалуйста заполните все данные!");
                        return false;
                    }
                    break;
            }
            var data = {};

            data["type"] = this.state.choosenType;
            data["status"] = 0;
            data["districts"] = [1, 2, 3, 4, 5, 6, 7, 8];
            data["location_x"] = "-62.91530";
            data["location_y"] = "-12.50813";

            switch (this.state.choosenType) {
                case 0:
                    if (this.state.atHome) {
                        data["type"] = 3;
                    }
                    data["specialities"] = [this.state.choosenDoctor.id];
                    data["services"] = [];
                    break;
                case 1:
                    data["specialities"] = [];
                    data["services"] = [this.state.choosenProcedure.id];
                    break;
                case 2:
                    data["specialities"] = [];
                    data["services"] = [this.state.choosenSurvey.id];
                    break;
                case 3:
                    data["specialities"] = [this.state.choosenDentist.id];
                    data["services"] = [];
                    data["type"] = 4;
                    break;
                case 4:
                    data["specialities"] = [];
                    data["services"] = [this.state.choosenDentistProcedure.id];
                    data["type"] = 5;
            }
            console.log(data, "ALL DATA");
            _DoctorService2.default.createRequest(data).then(function (response) {
                console.log(response, "response create request");
                var name = '';
                switch (_this7.state.choosenType) {
                    case 0:
                        name = _this7.state.choosenDoctor.name;
                        break;
                    case 1:
                        name = _this7.state.choosenProcedure.name;
                        break;
                    case 2:
                        name = _this7.state.choosenSurvey.name;
                        break;
                };
                _this7.props.navigation.navigate('resultRequest', {
                    id: response,
                    name: name,
                    city: _this7.state.choosenCity
                });
            }, function (error) {
                console.log(error.response, "error response");
            });
        }
