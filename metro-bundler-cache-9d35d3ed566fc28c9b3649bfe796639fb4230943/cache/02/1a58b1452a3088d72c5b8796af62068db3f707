    }, {
        key: "searchDoctor",
        value: function searchDoctor() {
            var _this7 = this;

            switch (this.state.choosenType) {
                case 0:
                    if (this.state.atHome) {
                        if (this.state.choosenCity.name === '' || this.state.choosenDoctor.name === '' || this.state.address === '') {
                            _reactNative.Alert.alert("Пожалуйста заполните все данные!");
                            return false;
                        }
